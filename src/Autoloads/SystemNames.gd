var availble_star_names: Array = DEFAULT_STAR_NAMES
var taken_star_names := []


func GetRandomName() -> String:
	return availble_star_names[randi() % availble_star_names.size()]


func ReserveName(name : String) -> bool:
	name = name.strip_edges()
	if __takenStarNames.has(name):
		return false
	elif __availStarNames.has(name):
		__availStarNames.erase(name)
	__takenStarNames.append(name)
	return true


func Reset():
	for name in __takenStarNames:
		__availStarNames.append(name)
		__takenStarNames.erase(name)
	__availStarNames.sort()
